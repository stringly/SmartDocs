@model IEnumerable<string>
@{
    Layout = "_Layout";
}
<h2>Current Job Descriptions</h2>
<a asp-action="Create">Add New</a>
<table class="table">
    <thead>
        <tr>
            <th>
                File Path
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @item
            </td>
            <td>                
                <a asp-action="Edit" asp-route-filePath="@item">Edit</a> |
                <a asp-action="Details" asp-route-id="@item">Details</a> |
                <a asp-action="Delete" asp-route-id="@item">Delete</a>
            </td>
        </tr>
        }
    </tbody>
</table>
